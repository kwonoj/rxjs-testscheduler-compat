{
  "name": "@kwonoj/rxjs-testscheduler-compat",
  "version": "1.0.1",
  "description": "RxJS v4 TestScheduler interface for RxJS v5",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=4.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "ci",
        "build"
      ]
    }
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "lint-staged": {
    "*.@(ts)": [
      "tslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "prepublish": "npm prune && npm run build",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg",
    "test": "npm-run-all build:clean && mocha --reporter nyan --compilers ts:ts-node/register --recursive --bail spec",
    "test:cover": "nyc --reporter=lcov --reporter=html npm test",
    "lint": "tslint src/**/*.ts spec/**/*.ts",
    "lint:staged": "lint-staged",
    "build": "npm-run-all build:clean && tsc",
    "build:clean": "shx rm -rf ./dist",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwonoj/rxjs-testscheduler-compat.git"
  },
  "keywords": [
    "Rx",
    "RxJS",
    "ReactiveX",
    "ReactiveExtensions",
    "Streams",
    "Observables",
    "Observable",
    "Stream",
    "ES6",
    "ES2015"
  ],
  "author": "OJ Kwon <kwon.ohjoong@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwonoj/rxjs-testscheduler-compat/issues"
  },
  "homepage": "https://github.com/kwonoj/rxjs-testscheduler-compat#readme",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/lodash": "^4.14.61",
    "@types/mocha": "^2.2.40",
    "chai": "^4.0.1",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.1",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "lodash": "^4.16.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "rxjs": "^5.2.0",
    "shx": "^0.2.0",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2",
    "validate-commit-msg": "^2.12.1"
  },
  "peerDependencies": {
    "rxjs": "5.x"
  }
}
